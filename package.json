{
  "name": "tanstack-start-starter",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "auth:generate": "pnpm dlx @better-auth/cli@latest generate --config ./src/lib/auth/server.ts --output ./src/lib/db/schemas/auth.ts",
    "build": "pnpm with-env vite build",
    "db:generate": "drizzle-kit generate --config drizzle.config.ts",
    "db:migrate": "pnpm with-env drizzle-kit migrate --config drizzle.config.ts",
    "db:reset": "pnpm with-env tsx src/lib/db/reset.ts",
    "db:seed": "pnpm with-env tsx src/lib/db/seed.ts",
    "db:studio": "pnpm with-env drizzle-kit studio --config drizzle.config.ts",
    "dev": "pnpm with-env vite",
    "env:login": "pnpm dlx dotenv-vault@latest login --yes",
    "env:open": "pnpm dlx dotenv-vault@latest open --yes",
    "env:pull": "pnpm dlx dotenv-vault@latest pull",
    "env:push": "pnpm dlx dotenv-vault@latest push",
    "format": "prettier -uwl --cache .",
    "format:check": "prettier -uc --cache .",
    "lint": "eslint . --max-warnings 0 --cache --cache-location node_modules/.cache/eslint/.eslintcache",
    "lint:fix": "eslint . --fix --cache --cache-location node_modules/.cache/eslint/.eslintcache",
    "prepare": "husky",
    "start": "pnpm with-env node .output/server/index.mjs",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenvx run --"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.48.4",
    "@hookform/resolvers": "^5.2.1",
    "@react-email/components": "^0.3.3",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-router": "^1.130.9",
    "@tanstack/react-router-with-query": "^1.130.9",
    "@tanstack/react-start": "^1.130.9",
    "arktype": "^2.1.20",
    "better-auth": "^1.3.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "drizzle-arktype": "^0.1.3",
    "drizzle-orm": "^0.44.4",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.534.0",
    "nanoid": "^5.1.5",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "pino": "^9.7.0",
    "pino-pretty": "^13.1.1",
    "radix-ui": "^1.4.2",
    "react": "^19.1.1",
    "react-day-picker": "^9.8.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.61.1",
    "react-intersection-observer": "^9.16.0",
    "react-resizable-panels": "^3.0.4",
    "recharts": "^2.15.4",
    "resend": "^4.7.0",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@faker-js/faker": "^9.9.0",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/eslint-plugin-query": "^5.81.2",
    "@tanstack/eslint-plugin-router": "^1.129.7",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@tanstack/react-router-devtools": "^1.130.9",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^24.1.0",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "drizzle-kit": "^0.31.4",
    "drizzle-seed": "^0.3.1",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^60.0.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.6",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.6",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad",
  "engines": {
    "node": ">=22.17.1"
  }
}
