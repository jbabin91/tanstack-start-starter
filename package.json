{
  "name": "tanstack-start-starter",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "auth:generate": "pnpm dlx @better-auth/cli@latest generate --config ./src/lib/auth/server.ts --output ./src/lib/db/schemas/auth.ts",
    "build": "pnpm with-env vite build",
    "build-storybook": "storybook build",
    "commit": "cz",
    "commit:lint": "commitlint --edit",
    "db:generate": "drizzle-kit generate --config drizzle.config.ts",
    "db:migrate": "pnpm with-env drizzle-kit migrate --config drizzle.config.ts",
    "db:reset": "pnpm with-env tsx src/lib/db/reset.ts",
    "db:seed": "pnpm with-env tsx src/lib/db/seed/index.ts",
    "db:studio": "pnpm with-env drizzle-kit studio --config drizzle.config.ts",
    "dev": "pnpm with-env vite",
    "env:login": "pnpm dlx dotenv-vault@latest login --yes",
    "env:open": "pnpm dlx dotenv-vault@latest open --yes",
    "env:pull": "pnpm dlx dotenv-vault@latest pull",
    "env:push": "pnpm dlx dotenv-vault@latest push",
    "format": "prettier -uwl --cache .",
    "format:check": "prettier -uc --cache .",
    "lint": "eslint . --max-warnings 0 --cache --cache-location node_modules/.cache/eslint/.eslintcache",
    "lint:fix": "eslint . --fix --cache --cache-location node_modules/.cache/eslint/.eslintcache",
    "lint:md": "markdownlint-cli2",
    "lint:md:fix": "markdownlint-cli2 --fix",
    "start": "pnpm with-env pnpm srvx --prod -s ../client dist/server/server.js",
    "storybook": "storybook dev -p 6006",
    "test": "pnpm with-env vitest --project=unit run",
    "test:all": "pnpm with-env vitest run",
    "test:coverage": "pnpm with-env vitest --project=unit run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:storybook": "pnpm with-env vitest --project=storybook --run",
    "test:ui": "pnpm with-env vitest --project=unit --ui",
    "test:watch": "pnpm with-env vitest --project=unit",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenvx run --"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.901.0",
    "@base-ui-components/react": "1.0.0-beta.4",
    "@dotenvx/dotenvx": "^1.51.0",
    "@hookform/resolvers": "^5.2.2",
    "@react-email/components": "^0.5.6",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-router": "^1.132.47",
    "@tanstack/react-router-ssr-query": "^1.132.47",
    "@tanstack/react-start": "^1.132.48",
    "arktype": "^2.1.22",
    "better-auth": "^1.3.26",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "drizzle-arktype": "^0.1.3",
    "drizzle-orm": "^0.44.6",
    "embla-carousel-react": "^8.6.0",
    "geolite2-redist": "^3.1.1",
    "input-otp": "^1.4.2",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.545.0",
    "maxmind": "^5.0.0",
    "nanoid": "^5.1.6",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "pino": "^10.0.0",
    "pino-pretty": "^13.1.1",
    "radix-ui": "^1.4.3",
    "react": "^19.2.0",
    "react-day-picker": "^9.11.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.64.0",
    "react-intersection-observer": "^9.16.0",
    "react-resizable-panels": "^3.0.6",
    "recharts": "^2.15.4",
    "resend": "^6.1.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "ua-parser-js": "^2.0.5",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
